        </files>
      </component>
    </bundle-->
  </components>

  <boards>
	<board vendor="GreeMicroelectronics" name="GM6601A Demoboard" revision="1.0.0">
      <description>GreeMicroelectronics GM6601A demo board</description>
      <!--book category="overview"  name="http://www.gree.com/" title="GM6601 demoboard Web Page"/-->
      <mountedDevice    deviceIndex="0" Dvendor="GreeMicroelectronics:193" Dname="GM6601ALQFP64"/>
      <compatibleDevice deviceIndex="0" Dvendor="GreeMicroelectronics:193" DsubFamily="GM6601"/>
      <feature type="XTAL"            n="8000000"/>
      <feature type="PWR"             n="5"              name="Adapter power input"/>
      <feature type="PWR"             n="5"              name="USB power input"/>
      <!--feature type="UART"           n="1"/-->
      <!--feature type="IR"              n="1"/-->
      <!--feature type="ADC"             n="1"              name="16 channels input"/-->
      <!--feature type="ACMP"            n="1"              name="Three channels input"/-->
      <!--feature type="EEPROM"          n="1"              name="I2C EEPROM"/-->
      <feature type="Button"          n="1"              name="Buttons for Reset"/>
      <feature type="LED"             n="2"              name="LEDs directly connected to port pins"/>
      <debugInterface adapter="SWD" connector="1*4 pin SWD (2.54 mm connector)"/>
    </board>
  </boards>
  
